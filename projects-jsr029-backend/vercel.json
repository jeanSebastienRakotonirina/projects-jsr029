{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "../projects-jsr029-frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "../projects-jsr029-frontend/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "../projects-jsr029-frontend/build/$1"
      }
    ]
  }
  